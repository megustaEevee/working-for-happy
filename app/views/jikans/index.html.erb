<div class="main">

<div class="left_bar">
<h2>メンバー</h2>

  <% if user_signed_in? %>
    <%= link_to current_user.username, "#", class: "username" %>
    <%= link_to 'ログアウト', destroy_user_session_path, method: :delete, class: "logout" %>
  <% else %>
    <%= link_to "新規登録", new_user_registration_path, class: "login" %>
    <%= link_to "ログイン", new_user_session_path, class: "sign-up" %>
  <% end %>

  <div calss="member">
    <ul>
      <% @user.each do |n| %>
        <li><%= link_to n.username %></li> 
      <% end %>
    </ul>
  </div>
</div>

<div class="right_bar">
</div>

</div>